export async function getStaticPaths() {
	return [
		{ params: { slug : 'bignumbers' } }, 
		{ params: { slug : 'blades' } },
		{ params: { slug : 'colorfinder' } },
		{ params: { slug : 'colorthing' } },
		{ params: { slug : 'fancyrecorder' } },
		{ params: { slug : 'kidthing' } },
		{ params: { slug : 'live-cams' } },
		{ params: { slug : 'music2' } },
		{ params: { slug : 'nightnight' } },
		{ params: { slug : 'recorder' } },
		{ params: { slug : 'recordergame' } }, 
		{ params: { slug : 'speedup' } }, 
		{ params: { slug : 'speedup-fard' } }, 
		{ params: { slug : 'spin' } },
		{ params: { slug : 'timer4' } },
		{ params: { slug : 'voicemaker' } },
		{ params: { slug : 'timer2' } },
		{ params: { slug : 'timer3' } }
	]
}

const { slug } = Astro.params;
const { View, title } = Astro.props;
---

<Layout title=`${ title }`>
		{ slug == 'bignumbers' && <BigNumbers client:only="preact" /> }
		{ slug == 'blades' && <Blades client:only="preact" /> }
		{ slug == 'colorfinder' && <ColorFinder client:only="preact" /> }
		{ slug == 'colorthing' && <ColorThing client:only="preact" /> }
		{ slug == 'fancyrecorder' && <FancyRecorder client:only="preact" /> }
		{ slug == 'kidthing' && <KidThing client:only="preact" /> }
		{ slug == 'live-cams' && <LiveCams client:only="preact" /> }
		{ slug == 'music2' && <Music2 client:only="preact" /> }
		{ slug == 'nightnight' && <NightNight client:only="preact" /> }
		{ slug == 'recorder' && <Recorder client:only="preact" /> }
		{ slug == 'recordergame' && <RecorderGame client:only="preact" /> }
		{ slug == 'speedup' && <SpeedUp client:load /> }
		{ slug == 'speedup-fard' && <SpeedUpFard client:only="preact" /> }
		{ slug == 'spin' && <Spin client:only="preact" /> }
		{ slug == 'voicemaker' && <VoiceMaker client:only="preact" /> }
		{ slug == 'timer2' && <Timer2 client:only="preact" /> }
		{ slug == 'timer3' && <Timer3 client:only="preact" /> }
		{ slug == 'timer4' && <Timer4 client:only="preact" /> }
</Layout>

